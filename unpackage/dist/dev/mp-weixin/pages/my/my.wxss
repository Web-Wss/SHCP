/* 水平间距 */
.image-name {
  padding: 20rpx;
  display: flex;
  width: 100%;
  height: 100rpx;
  background-color: white;
}
.image-name .image {
  border-radius: 25px;
  width: 100rpx;
  height: 100rpx;
}
.image-name .image image {
  border-radius: 25px;
  width: 100rpx;
  height: 100rpx;
}
.image-name .name {
  margin-left: 20rpx;
  line-height: 100rpx;
}
.publish-collection {
  display: flex;
  flex: 1;
  flex-direction: row;
  flex-wrap: nowrap;
  margin-top: 15rpx;
  width: 100%;
  height: 180rpx;
}
.publish-collection .two {
  border-radius: 15px;
  margin: 0rpx 9rpx;
  width: 48%;
  height: 180rpx;
  background-color: white;
}
.publish-collection .two .image-icon {
  margin-top: 20rpx;
  text-align: center;
}
.publish-collection .two .text {
  text-align: center;
  font-size: 40rpx;
}

